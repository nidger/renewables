{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strict": false,
    "noEmit": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}